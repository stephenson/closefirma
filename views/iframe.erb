<% if @leads["data"].count > 0 %>
  <% @leads["data"].each do |lead| %>
    <div class="section">
      <p>
        <strong>Lead: </strong>
        <a href="<%= lead["html_url"] %>" target="_blank"><%= lead["display_name"] %></a>
      </p>
    </div>
  
    <div class="section">
      <p>
        <strong>SÃ¦lger: </strong>
        <% lead["opportunities"].each do |op| %>
          <%= op["user_name"] %>
        <% end %>
      </p>
    </div>
  
    <div class="section">
      <p>
        <strong>Kontakter: </strong>
        <ul>
          <% lead["contacts"].each do |contact| %>
            <li><%= contact["name"] %></li>
          <% end %>
        </ul>
      </p>
    </div>
  <% end %>
<% else %>
  Der blev ikke fundet nogle leads.
<% end %>
